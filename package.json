  "main": "src/index.js",
  "dependencies": {
    "@mapbox/geojson-extent": "^0.3.2",
    "d3-request": "^1.0.5",
    "geojson-rewind": "^0.2.0",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.38.0",
    "object-assign": "^4.1.1",
    "sheetify": "^6.1.0",
    "yo-yo": "^1.4.1"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "budo": "^10.0.3",
    "copyfiles": "^1.2.0",
    "es2020": "^1.1.9",
    "gh-pages": "^1.0.0",
    "mkdirp": "^0.5.1",
    "standard": "^10.0.2",
    "uglify-es": "^3.0.24",
    "uglifyify": "kristoferjoseph/uglifyify#5881f398ec8b6b5cee2f65e0ee19480bd2db92b1",
    "yo-yoify": "^3.7.3"
  },
  "scripts": {
    "test": "standard",
    "start": "budo src/index.js:bundle.js --live -d static",
    "build": "mkdirp dist && npm run build:js && npm run build:static",
    "build:js": "browserify src/index.js -t es2020 -t varify -t sheetify -t yo-yoify | uglifyjs -c -m > dist/bundle.js",
    "build:static": "copyup 'static/**/*' dist && copyfiles data.json dist",
    "deploy": "npm run build && gh-pages -d dist"
  },
